[init]
	defaultBranch = main
[user]
	name = Alejandro
	email = alejandro.arancibia.aranda@gmail.com
[core]
	editor = nvim
	autocrlf = input
[color]
	ui = auto
[pull]
	rebase = true
	ff = only
[rebase]
	autoStash = true
[fetch]
	prune = true
[diff]
	algorithm = histogram
[help]
	autocorrect = 1
[branch]
	sort = -committerdate
[credential]
	helper = cache
[log]
	date = iso
